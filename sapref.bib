@inproceedings{Brookes2011,
  title = {Can {{Binary Masks Improve Intelligibility}}?},
  booktitle = {Proc {{Wkshp}} on {{Speech}} in {{Noise}}: {{Intelligibility}} and {{Quality}}},
  author = {Brookes, Mike and Huckvale, Mark},
  year = {2011},
  month = jan,
  address = {{Lyon}},
  abstract = {Previous work has claimed that a pattern classification method can be used to estimate a binary mask from a noisy speech signal to identify time-frequency regions containing significant speech energy. Speech recreated from these regions shows an increased intelligibility over the original signal. In this paper we present the results from a replication of the binary mask study by Kim, Lu, Hu \& Loizou (2009) in which we recreate the speech and noise materials, speech features, classifiers and intelligibility testing as closely as possible. We compare our findings with those of the original study, and discuss the reasons for the differences we found. One question that arises is whether the ``ideal binary mask'' is the best target for an enhancement algorithm. We will present preliminary results from an investigation of different local criteria for mask selection.},
