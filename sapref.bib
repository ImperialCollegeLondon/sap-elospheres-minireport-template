@article{Kerlin2010,
  title = {Attentional {{Gain Control}} of {{Ongoing Cortical Speech Representations}} in a "{{Cocktail Party}}"},
  author = {Kerlin, Jess R. and Shahin, Antoine J. and Miller, Lee M.},
  year = {2010},
  journal = J_NS,
  volume = {30},
  number = {2},
  pages = {620--628},
  issn = {0270-6474},
  doi = {10.1523/JNEUROSCI.3631-09.2010},
  abstract = {Normal listeners possess the remarkable perceptual ability to select a single speech stream among many competing talkers. However, few studies of selective attention have addressed the unique nature of speech as a temporally extended and complex auditory object. We hypothesized that sustained selective attention to speech in a multitalker environment would act as gain control on the early auditory cortical representations of speech. Using high-density electroencephalography and a template-matching analysis method, we found selective gain to the continuous speech content of an attended talker, greatest at a frequency of 4\textendash 8 Hz, in auditory cortex. In addition, the difference in alpha power (8\textendash 12 Hz) at parietal sites across hemispheres indicated the direction of auditory attention to speech, as has been previously found in visual tasks. The strength of this hemispheric alpha lateralization, in turn, predicted an individualtextquoterights attentional gain of the cortical speech signal. These results support a model of spatial speech stream segregation, mediated by a supramodal attention mechanism, enabling selection of the attended representation in auditory cortex.}
}

@article{Keshavarz2012,
