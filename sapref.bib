  author = {Haddad, D. Barreto and Petraglia, M. Rembold and Batalheiro, P. Bulkool},
  year = {2013},
  month = aug,
  pages = {153--157},
  doi = {10.1109/DSP-SPE.2013.6642582},
  abstract = {Blind separation techniques of sound sources, designed to work with voice signals, present a performance highly dependent on the number of coefficients of the separation system. In general, different environments require different lengths of separation filters. This paper proposes the use of reverberation time information arising from lateral blind estimation techniques for tuning the degree of freedom of the separation system in order to blindly obtain a reasonable source-to-distortion ratio (SDR). This tuning enables, for example, that the complexity of the separation system be adjusted in each band if a subband structure is employed for sources separation.},
  owner = {dje11},
  keywords = {acoustic generators,acoustic signal processing,acoustic sources,Blind Estimation of Reverberation Time,blind separation techniques,blind source separation,Conferences,degree-of-freedom,Estimation,filtering theory,lateral blind estimation techniques,Measurement,reverberation,reverberation time estimation,SDR,separation filter lengths,separation system coefficients,separation system complexity,sound sources,source separation,source-to-distortion ratio,Speech,speech processing,subband structure,Subband Structures,voice signals}
