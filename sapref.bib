@phdthesis{Huber2003,
  title = {Objective Assessment of Audio Quality Using an Auditory Processing Model},
  author = {Huber, Rainer},
  year = {2003},
  month = dec,
  url = {http://docserver.bis.uni-oldenburg.de/publikationen/dissertation/2004/hubobj03/pdf/hubobj03.pdf},
  abstract = {In this thesis, a novel computational method for the perceptual evaluation of quality differences between audio signals is presented. The core of this method is the computer model of the ``effective'' signal processing in the auditory system by Dau et al. (1997) which is employed to transform the acoustic signals that are to be compared, into corresponding internal representations in the perceptual domain. Chapter 2 shows the capability of the method to predict subjective quality ratings of audio signals which have been processed by different lossy audio coding schemes. In chapter 3, an alternative method for the subjective evaluation of high quality audio coding schemes is proposed, which estimates detection thresholds of audio distortions psychoacoustically. The simulation of the performed psychoacoustic experiments by the auditory model of Dau et al. yields high correlation with the measured data. Finally, chapter 4 investigates how the perceived quality of noise reduction schemes for speech can be predicted by means of the method presented in chapter 2. To complete the picture optimized variants of the prediction method are included in the study as well as different subjective quality criteria to be predicted by these methods.},
  owner = {dmb},
  school = INST_UO
}

