@inproceedings{Eaton2017,
  title = {Estimation of the Perceived Level of Reverberation Using Non-Intrusive Single-Channel Variance of Decay Rates},
  booktitle = HSCMA,
  author = {Eaton, James and Javed, Hamza A. and Naylor, Patrick A.},
  year = {March 2017, Submitted},
  address = {{San Francisco, California, USA}},
  abstract = {The increasing processing power of hearing aids and mobile devices has led to the potential for incorporation of dereverberation algorithms to improve speech quality for the listener. Assessing the effectiveness of deverberation algorithms using subjective listening tests is extremely time consuming and depends on averaging out listener variations over a large number of subjects. Also, most existing instrumental measures are intrusive and require knowledge of the original signal which precludes many practical applications. In this paper we show that the proposed non-intrusive single-channel algorithm is a predictor of the perceived level of reverberation that correlates well with subjective listening test results, outperforming many existing intrusive and non-intrusive measures. The algorithm requires only a single training step and has a very low computational complexity making it suitable for hearing aids and mobile telephone applications. The source code has been made freely available.},
